  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.4",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.19.2",
    "sequelize-typescript": "^2.1.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.35",
    "@types/validator": "^13.7.2",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "npm run build && node --unhandled-rejections=strict ./dist/src/index.js",
    "debug": "export DEBUG=* && npm start",
    "lint": "tslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint --fix",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "pretest": "NODE_ENV=test npx sequelize-cli db:migrate",
    "test": "NODE_ENV=test jest --runInBand --config ./test/jest-e2e.json",
    "posttest": "NODE_ENV=test npx sequelize-cli db:migrate:undo:all"
  }
}
